<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <TextView
            android:text="Cambiado!"
            android:layout_width="82dp"
            android:layout_height="32dp"
            android:id="@+id/prueba" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:gravity="center_horizontal|center_vertical" app:layout_constraintHorizontal_bias="0.111"
            app:layout_constraintVertical_bias="0.248"/>
    <TextView
            android:text="Componentes"
            android:layout_width="96dp"
            android:layout_height="28dp"
            android:id="@+id/components" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.093" android:layout_marginTop="136dp"
            app:layout_constraintTop_toTopOf="parent"
            android:gravity="center" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/prueba"/>
    <TextView
            android:layout_width="82dp"
            android:layout_height="32dp"
            android:id="@+id/material"
            app:layout_constraintTop_toBottomOf="@+id/prueba" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.111"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"/>
</android.support.constraint.ConstraintLayout>